---
import Layout from "../layout/Layout.astro";
import { CanvasContainer } from "../canvas/CanvasContainer";

const title = "3D Tetris";
const description = "3D Tetris";
const permalink = Astro.url.href;
---

<Layout title={title} description={description} permalink={permalink}>
	<div class="w-full min-h-screen h-screen relative overflow-hidden">
		<CanvasContainer client:only="react" />
	</div>
</Layout>
